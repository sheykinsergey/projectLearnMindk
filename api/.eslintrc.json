{
    "env": {
        "browser": true,
        "es2021": true,
        "node": true
    },
    "extends": "airbnb-base",
    "parserOptions": {
        "ecmaVersion": 13,
        "sourceType": "module"
    },
    "rules": {
        "no-console": "warn",
        "semi": "warn",
        "max-len": ["error", 80, {"ignoreUrls": true}]
    }
}
